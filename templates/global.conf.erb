zodb-db-type mysql
zodb-host <%=zenoss_db_host%>
zodb-port 3306
zodb-db zodb
zodb-user <%=zenoss_db_user%>
zodb-password <%=zenoss_db_password%>
zodb-admin-user <%=zenoss_db_admin_user%>
zodb-admin-password <%=zenoss_db_admin_password%>
zodb-cacheservers 127.0.0.1:11211
zodb-cachesize 1000

amqphost localhost
amqpport 5672
amqpvhost /zenoss
amqpuser zenoss
amqppassword zenoss
amqpusessl 0
amqpadminport 55672
amqpadminusessl 0

zep-db-type mysql
zep-host <%=zenoss_db_host%>
zep-port 3306
zep-db zenoss_zep
zep-user <%=zenoss_db_user%>
zep-password <%=zenoss_db_password%>
zep-admin-user <%=zenoss_db_admin_user%>
zep-admin-password <%=zenoss_db_admin_password%>
zep-uri http://localhost:8084
